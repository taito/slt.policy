<?xml version="1.0"?>
<registry>
  <record name="collective.cart.shopping.notification_cc_email">
    <value>lk-tilaukset@sll.fi</value>
  </record>
  <record name="hexagonit.socialbutton.config">
    <field type="plone.registry.field.Dict">
      <title>Configuration for Social Buttons</title>
      <key_type type="plone.registry.field.TextLine" />
      <value_type type="plone.registry.field.Dict">
        <title>Value list</title>
        <key_type type="plone.registry.field.TextLine" />
        <value_type type="plone.registry.field.Text">
          <title>Values</title>
        </value_type>
      </value_type>
    </field>
    <value>
      <element key="twitter">
        <element key="view_permission_only">True</element>
        <element key="enabled">True</element>
        <element key="view_models">*</element>
        <element key="content_types">Document,Folder,FormFolder,Plone Site,News Item,Event,collective.cart.core.Article,collective.cart.shopping.ArticleContainer</element>
        <element key="viewlet_manager">plone.belowcontent</element>
      </element>
      <element key="facebook">
        <element key="view_permission_only">True</element>
        <element key="enabled">True</element>
        <element key="view_models">*</element>
        <element key="content_types">Document,Folder,FormFolder,Plone Site,News Item,Event,collective.cart.core.Article,collective.cart.shopping.ArticleContainer</element>
        <element key="viewlet_manager">plone.belowcontent</element>
      </element>
      <element key="google-plus">
        <element key="view_permission_only">True</element>
        <element key="enabled">True</element>
        <element key="view_models">*</element>
        <element key="content_types">Document,Folder,FormFolder,Plone Site,News Item,Event,collective.cart.core.Article,collective.cart.shopping.ArticleContainer</element>
        <element key="viewlet_manager">plone.belowcontent</element>
      </element>
    </value>
  </record>
</registry>